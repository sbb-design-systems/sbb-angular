    "@types/geojson": "7946.0.10",
    "@types/glob": "8.0.0",
    "@types/grecaptcha": "2.0.36",
    "@types/highlight.js": "10.1.0",
    "@types/jasmine": "4.3.0",
    "@types/jasminewd2": "2.0.10",
    "@types/mapbox__point-geometry": "0.1.2",
    "@types/marked": "^2.0.2",
    "@types/node": "16.18.3",
    "@types/parse5": "6.0.3",
    "@types/sass": "1.43.1",
    "@types/send": "0.17.1",
    "@types/yargs": "17.0.13",
    "angular-server-side-configuration": "15.0.1",
    "angular-t9n": "15.0.0",
    "browser-sync": "2.23.6",
    "codelyzer": "6.0.2",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.29.3",
    "esbuild": "0.15.15",
    "fs-extra": "10.1.0",
    "highlight.js": "^10.7.0",
    "history-server": "1.3.1",
    "html-insert-assets": "0.14.3",
    "husky": "8.0.2",
    "jasmine": "4.5.0",
    "jasmine-core": "4.5.0",
    "jsonc-parser": "3.2.0",
    "karma": "6.4.1",
    "karma-browserstack-launcher": "1.6.0",
