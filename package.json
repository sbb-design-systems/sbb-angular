    "@rollup/plugin-commonjs": "23.0.4",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "11.1.5",
    "@schematics/angular": "17.0.3",
    "@types/browser-sync": "2.27.5",
    "@types/events": "3.0.3",
    "@types/fs-extra": "11.0.4",
    "@types/geojson": "7946.0.13",
    "@types/glob": "8.1.0",
    "@types/grecaptcha": "2.0.36",
    "@types/highlight.js": "10.1.0",
    "@types/jasmine": "5.1.4",
    "@types/jasminewd2": "2.0.13",
    "@types/mapbox__point-geometry": "0.1.4",
    "@types/marked": "^2.0.2",
    "@types/node": "18.18.13",
    "@types/sass": "1.43.1",
    "@types/send": "0.17.4",
    "@types/yargs": "17.0.13",
    "angular-server-side-configuration": "17.0.2",
    "angular-t9n": "17.0.0",
    "browser-sync": "2.23.6",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.29.3",
    "esbuild": "0.19.7",
    "fs-extra": "11.1.1",
    "highlight.js": "^11.8.0",
    "history-server": "1.3.1",
    "html-insert-assets": "0.14.3",
    "husky": "8.0.3",
    "jasmine": "4.6.0",
    "jasmine-core": "4.6.0",
    "jsonc-parser": "3.2.0",
    "karma": "6.4.2",
    "karma-browserstack-launcher": "1.6.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-firefox-launcher": "2.1.2",
    "karma-jasmine": "5.1.0",
    "karma-junit-reporter": "2.0.1",
    "karma-parallel": "0.3.1",
    "karma-requirejs": "1.1.0",
    "karma-sourcemap-loader": "0.4.0",
    "lint-staged": "15.1.0",
    "marked": "^2.0.3",
    "npm-run-all": "4.1.5",
    "octokit": "3.1.2",
    "patch-package": "^8.0.0",
    "prettier": "3.1.0",
    "protractor": "7.0.0",
    "renovate": "37.37.2",
    "requirejs": "2.3.6",
    "rollup": "2.79.1",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rxjs-tslint-rules": "4.34.8",
    "sass": "1.69.5",
    "semver": "7.5.4",
    "send": "0.18.0",
    "shelljs": "0.8.5",
    "standard-version": "9.5.0",
    "stylelint": "15.11.0",
    "stylelint-config-recommended-scss": "13.1.0",
    "stylelint-scss": "5.3.1",
    "terser": "5.24.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "1.16.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "5.2.2",
    "yargs": "17.7.2"
  },
  "resolutions": {
    "@angular/build-tooling/typescript": "5.2.2",
    "@angular/ng-dev/typescript": "5.2.2",
    "browser-sync-client": "2.26.13",
    "dgeni-packages/typescript": "5.2.2"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      },
      {
        "type": "style",
        "section": "Bug Fixes"
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "lf",
    "printWidth": 100
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "rules": {
      "no-empty-source": null,
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "ng-deep"
          ]
        }
      ],
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^sbb-/"
          ]
        }
      ],
      "no-descending-specificity": null
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        0
      ],
      "footer-max-line-length": [
        0,
        "always",
        0
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,css,scss,json,md,html}": [
      "prettier --write"
    ],
    "*.ts": [
      "tslint -c tslint.json --project ./tsconfig.json --fix"
    ],
    "*.{bzl,bazel}": [
      "buildifier -v"
    ]
  }
}
