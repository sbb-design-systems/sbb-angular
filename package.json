    "@angular/ng-dev": "https://github.com/angular/dev-infra-private-ng-dev-builds.git#183ef3004a595de30b14bd9b736034d9c57f20e3",
    "@babel/core": "7.20.2",
    "@babel/traverse": "7.20.1",
    "@bazel/bazelisk": "1.12.1",
    "@bazel/buildifier": "5.1.0",
