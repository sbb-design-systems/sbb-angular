    "@types/highlight.js": "10.1.0",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/marked": "^2.0.2",
    "@types/node": "16.11.25",
    "@types/parse5": "6.0.3",
    "@types/sass": "1.43.1",
    "@types/send": "0.17.1",
    "@types/yargs": "17.0.8",
    "angular-t9n": "13.0.0",
    "browser-sync": "2.23.6",
    "codelyzer": "6.0.2",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.29.3",
    "esbuild": "0.14.23",
    "fs-extra": "10.0.0",
    "highlight.js": "^10.7.0",
    "history-server": "1.3.1",
    "html-insert-assets": "0.14.2",
    "husky": "7.0.4",
    "jasmine": "3.99.0",
    "jasmine-core": "3.99.0",
    "jsonc-parser": "3.0.0",
    "karma": "6.3.16",
    "karma-browserstack-launcher": "1.6.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.0",
    "karma-firefox-launcher": "2.1.2",
    "karma-jasmine": "4.0.1",
    "karma-junit-reporter": "2.0.1",
    "karma-parallel": "0.3.1",
    "karma-requirejs": "1.1.0",
    "karma-sourcemap-loader": "0.3.8",
    "lint-staged": "12.3.4",
    "marked": "^2.0.3",
    "npm-run-all": "4.1.5",
    "octokit": "1.7.1",
    "prettier": "2.5.1",
    "protractor": "7.0.0",
    "requirejs": "2.3.6",
    "rollup": "2.67.3",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rxjs-tslint-rules": "4.34.8",
    "sass": "1.49.8",
    "scss-bundle": "3.1.2",
    "semver": "7.3.5",
    "send": "0.17.2",
    "shelljs": "0.8.5",
    "standard-version": "9.3.2",
    "stylelint": "14.5.1",
    "stylelint-config-recommended-scss": "5.0.2",
    "stylelint-scss": "4.1.0",
    "terser": "5.10.0",
    "ts-node": "10.5.0",
    "tsconfig-paths": "3.12.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "1.16.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "~4.5.5",
    "yargs": "17.3.1"
  },
  "resolutions": {
    "@angular/dev-infra-private/typescript": "~4.5.5",
    "browser-sync-client": "2.26.13",
    "dgeni-packages/typescript": "4.5.5"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      },
      {
        "type": "style",
        "section": "Bug Fixes"
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "lf",
    "printWidth": 100
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "rules": {
      "no-empty-source": null,
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "ng-deep"
          ]
        }
      ],
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^sbb-/"
          ]
        }
      ],
      "no-descending-specificity": null
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        0
      ]
    }
  },
  "lint-staged": {
    "*.{js,ts,css,scss,json,md,html}": [
      "prettier --write"
    ],
    "*.ts": [
      "tslint -c tslint.json --project ./tsconfig.json --fix"
    ],
    "*.{bzl,bazel}": [
      "buildifier -v"
    ]
  }
}
